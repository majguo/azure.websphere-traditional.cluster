{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "_artifactsLocation": {
            "value": "${artifactsLocationBase}/${project.artifactId}/${git.tag}/src/main/"
        },
        "ibmUserId": {
            "value": "${ibmUserId}"
        },
        "ibmUserPwd": {
            "value": "${ibmUserPwd}"
        },
        "twasndVmImageId": {
            "value": "${twasndVmImageId}"
        },
        "numberOfNodes": {
            "value": "${numberOfNodes}"
        },
        "vmSize": {
            "value": "${vmSize}"
        },
        "dmgrVMPrefix": {
            "value": "${dmgrVMPrefix}"
        },
        "managedVMPrefix": {
            "value": "${managedVMPrefix}"
        },
        "dnsLabelPrefix":{
            "value": "${dnsLabelPrefix}"
        },
        "adminUsername": {
            "value": "${adminUsername}"
        },
        "adminPasswordOrKey": {
            "value": "${adminPasswordOrKey}"
        },
        "authenticationType": {
            "value": "${authenticationType}"
        },
        "wasUsername": {
            "value": "${wasUsername}"
        },
        "wasPassword": {
            "value": "${wasPassword}"
        },
        "configureIHS": {
            "value": "${configureIHS}"
        },
        "ihsVmImageId": {
            "value": "${ihsVmImageId}"
        },
        "ihsVmSize": {
            "value": "${ihsVmSize}"
        },
        "ihsVMPrefix": {
            "value": "${ihsVMPrefix}"
        },
        "ihsDnsLabelPrefix":{
            "value": "${ihsDnsLabelPrefix}"
        },
        "ihsUnixUsername": {
            "value": "${ihsUnixUsername}"
        },
        "ihsUnixPasswordOrKey": {
            "value": "${ihsUnixPasswordOrKey}"
        },
        "ihsAuthenticationType": {
            "value": "${ihsAuthenticationType}"
        },
        "ihsAdminUsername": {
            "value": "${ihsAdminUsername}"
        },
        "ihsAdminPassword": {
            "value": "${ihsAdminPassword}"
        }
    }
}